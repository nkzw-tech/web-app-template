{
  "name": "@nkzw/web-app-template",
  "version": "0.0.1",
  "author": "Christoph Nakazawa <christoph.pojer@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/nkzw-tech/web-app-template.git"
  },
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "dev:setup": "pnpm relay && pnpm fbtee:all",
    "fbtee:all": "pnpm fbtee collect && pnpm fbtee translate",
    "format": "oxfmt",
    "lint": "oxlint .",
    "lint:format": "oxfmt --check",
    "preinstall": "command -v git >/dev/null 2>&1 && git config core.hooksPath git-hooks || exit 0",
    "relay": "pnpm relay-compiler",
    "test": "npm-run-all --parallel tsc:check vitest:run lint lint:format",
    "tsc:check": "tsgo",
    "vitest:run": "vitest run"
  },
  "dependencies": {
    "@nkzw/core": "^1.3.1",
    "@nkzw/define-env": "^1.1.0",
    "@nkzw/stack": "^2.3.2",
    "better-auth": "^1.4.18",
    "fbtee": "^1.7.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.0.0",
    "react-relay": "^20.1.1",
    "react-router": "^7.13.0",
    "relay-runtime": "^20.1.1"
  },
  "devDependencies": {
    "@nkzw/babel-preset-fbtee": "^1.7.1",
    "@nkzw/eslint-plugin": "^2.0.0",
    "@nkzw/oxlint-config": "^1.0.1",
    "@swc/core": "^1.15.11",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^20.1.0",
    "@typescript/native-preview": "7.0.0-dev.20260212.1",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-react-compiler": "^1.0.0",
    "babel-plugin-relay": "^20.1.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.4.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0",
    "relay-compiler": "^20.1.1",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=23.0.0",
    "pnpm": ">=10.0.0"
  }
}
